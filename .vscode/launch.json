  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run npm dev in terminal",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
    },
    {
      "name": "Run Backend Server",
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/server.js",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev:server"],
      "console": "integratedTerminal"
    }
  ]
}